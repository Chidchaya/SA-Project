<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="timetable.controllers.StartController">
   <children>
      <ImageView fx:id="image" fitHeight="626.0" fitWidth="900.0" layoutY="-1.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="loginBtn" layoutX="295.0" layoutY="491.0" mnemonicParsing="false" onAction="#handleLoginBtnOnAction" prefHeight="51.0" prefWidth="310.0" text="Login" textAlignment="CENTER">
         <font>
            <Font name="System Bold Italic" size="24.0" />
         </font>
         <effect>
            <DropShadow color="WHITE" />
         </effect>
      </Button>
      <TextField fx:id="usernameTextField" layoutX="405.0" layoutY="384.0" prefHeight="31.0" prefWidth="187.0">
         <font>
            <Font name="System Bold Italic" size="15.0" />
         </font></TextField>
      <PasswordField fx:id="passwordField" layoutX="405.0" layoutY="433.0" prefWidth="187.0">
         <font>
            <Font name="System Bold Italic" size="15.0" />
         </font></PasswordField>
      <Label layoutX="310.0" layoutY="389.0" text="User name">
         <font>
            <Font name="System Bold Italic" size="15.0" />
         </font>
      </Label>
      <Label layoutX="310.0" layoutY="438.0" text="Password">
         <font>
            <Font name="System Bold Italic" size="15.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
